<% if defined?(@timeout) -%>
GRUB_TIMEOUT=<%= @timeout %>
<% end -%>
<% if defined?(@hidden_timeout) -%>
GRUB_HIDDEN_TIMEOUT=<%= @hidden_timeout %>
<% end -%>
<% if defined?(@_hidden_timeout_quiet) -%>
GRUB_HIDDEN_TIMEOUT_QUIET=<%= @_hidden_timeout_quiet %>
<% end -%>
<% if defined?(@default) -%>
GRUB_DEFAULT="<%= @default %>"
<% end -%>
<% if defined?(@_savedefault) -%>
GRUB_SAVEDEFAULT=<%= @_savedefault %>
<% end -%>
<% if defined?(@background) -%>
GRUB_BACKGROUND="<%= @background %>"
<% end -%>
<% if defined?(@_disable_submenu) -%>
GRUB_DISABLE_SUBMENU=<%= @_disable_submenu %>
<% end -%>
<% if defined?(@_disable_os_prober) -%>
GRUB_DISABLE_OS_PROBER=<%= @_disable_os_prober %>
<% end -%>
<% if defined?(@serial_command) -%>
GRUB_SERIAL_COMMAND="<%= @serial_command %>"
<% end -%>
<% if defined?(@terminal) -%>
GRUB_TERMINAL="<%= @terminal %>"
<% end -%>
<% if defined?(@terminal_input) -%>
GRUB_TERMINAL_INPUT="<%= @terminal_input %>"
<% end -%>
<% if defined?(@terminal_output) -%>
GRUB_TERMINAL_OUTPUT="<%= @terminal_output %>"
<% end -%>
<% if defined?(@cmdline_linux_base) -%>
GRUB_CMDLINE_LINUX="<%if defined?(@all_cmdline_linux_extra) -%><% @all_cmdline_linux_extra.sort.each do |item| -%><%= item %> <% end -%><% end -%><%if defined?(@cmdline_linux_crashkernel) -%>crashkernel=<%= @cmdline_linux_crashkernel %> <% end -%><%= @cmdline_linux_base %>"
<% end -%>
<% if defined?(@_disable_recovery) -%>
GRUB_DISABLE_RECOVERY=<%= @_disable_recovery %>
<% end -%>
